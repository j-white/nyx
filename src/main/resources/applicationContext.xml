<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.2.xsd">
 
	<context:annotation-config />
 
	<bean id="affineFractalCodec" class="math.nyx.codecs.AffineFractalCodec"/>

	<bean id="affineKernel" class="math.nyx.codecs.AffineKernel"/>

	<bean id="linearPartitioningStrategy" class="math.nyx.framework.LinearPartitioningStrategy"/>
	
	<bean id="averagingDecimationStrategy" class="math.nyx.framework.AveragingDecimationStrategy"/>

</beans>
